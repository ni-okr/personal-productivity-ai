{
  "name": "personal-productivity-ai",
  "version": "0.1.0",
  "description": "Персональный ИИ-ассистент для продуктивности",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "npx jest",
    "test:unit": "npx jest tests/unit",
    "test:integration": "npx jest tests/integration",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:allure": "playwright test --reporter=allure-playwright",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
    "test:allure": "npm run test:e2e:allure && npm run allure:generate && npm run allure:open",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:migrate": "node tests/scripts/migrate-tests.js",
    "test:migrate:all": "node tests/scripts/migrate-all-tests.js",
    "test:framework": "npm run test:unit -- --testPathPattern='migrated'",
    "test:check": "node tests/scripts/check-framework-compliance.js",
    "test:fix": "node tests/scripts/fix-framework-violations.js",
    "test:validate": "node tests/scripts/validate-migration.js",
    "test:unit:ci": "npx jest tests/unit --coverage --ci",
    "test:integration:ci": "npx jest tests/integration --ci",
    "test:e2e:ci": "mkdir -p playwright-report allure-results && playwright test --reporter=line,html",
    "test:mock": "npx jest tests/unit/auth-mock.test.ts tests/unit/tasks-mock.test.ts tests/unit/subscription-mock.test.ts",
    "test:mock:ci": "npx jest tests/unit/auth-mock.test.ts tests/unit/tasks-mock.test.ts tests/unit/subscription-mock.test.ts --coverage --ci",
    "test:integration:mock": "npx jest tests/integration/mock-mode.test.ts",
    "test:integration:mock:ci": "npx jest tests/integration/mock-mode.test.ts --ci",
    "test:e2e:mock": "playwright test tests/e2e/mock-mode.spec.ts",
    "test:e2e:mock:ci": "mkdir -p playwright-report allure-results && playwright test tests/e2e/mock-mode.spec.ts --reporter=line,html",
    "security:check": "./scripts/security-check.sh",
    "security:update": "./scripts/update-dependencies.sh",
    "security:audit": "npm audit --audit-level=high",
    "security:audit:prod": "./scripts/audit-production.sh",
    "security:audit:dev": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix --force",
    "deps:update": "npm update && npm audit fix --force",
    "deps:outdated": "npm outdated"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@playwright/mcp": "^0.0.37",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "firecrawl-mcp": "^3.1.13",
    "framer-motion": "^11.0.0",
    "github-mcp-server": "^1.8.7",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "postcss": "^8.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.52.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.0",
    "time-mcp": "^1.0.4",
    "typescript": "^5.4.0",
    "zod": "^3.25.76",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.4.1",
    "dotenv": "^17.2.2",
    "esbuild": "^0.25.9",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^2.7.0",
    "path-to-regexp": "^8.3.0",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "ts-jest": "^29.4.2",
    "undici": "^7.16.0",
    "vercel": "48.0.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "productivity",
    "ai",
    "assistant",
    "automation",
    "planning",
    "pwa"
  ],
  "author": {
    "name": "Personal AI Team",
    "email": "hello@personal-ai.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ni-okr/personal-productivity-ai"
  }
}